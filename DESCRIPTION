Package: fatalcaranalysiisreports
Type: Package
Title: Fatal Car Accident Reporting System (FCARS)
Version: 0.1.0
Authors@R: c(
    person(given = "Person1",
           family = "Creator",
           role = c("aut", "cre"),
           email = "person1@fcars.com"),
